{
  "name": "rootstack-challenges",
  "version": "0.0.0",
  "description": "a code challenge for roostack",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "test:unit": "mocha -r ts-node/register src/**/*.test.ts src/domain/**/*.test.ts src/infrastructure/**/*.test.ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "migrate:mongo": "ts-node -r dotenv/config ./src/infrastructure/database/migration.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format"
    }
  },
  "repository": {
    "type": "git",
    "url": "github.com/davidenq/rootstack-challenges"
  },
  "author": "David Nunez <david.nunez.dev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.4",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "mongodb": "^4.2.2"
  }
}
